{
  "name": "free-nextjs-admin-dashboard",
  "version": "2.2.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "deploy": "npm run build && npx wrangler pages deploy .next --project-name=dubai-filmmaker-cms",
    "deploy:preview": "npm run build && npx wrangler pages deploy .next --project-name=dubai-filmmaker-cms --branch=preview",
    "db:setup": "node scripts/d1-setup.js",
    "db:migrate": "node scripts/d1-migrate.js",
    "db:migrate:local": "node scripts/d1-migrate.js --local",
    "db:seed": "node scripts/d1-seed.js",
    "db:seed:local": "node scripts/d1-seed.js --local",
    "db:studio": "wrangler d1 info dubai-filmmaker-cms --json",
    "db:console": "wrangler d1 execute dubai-filmmaker-cms --remote --command=\"SELECT * FROM projects LIMIT 5;\"",
    "db:console:local": "wrangler d1 execute dubai-filmmaker-cms --command=\"SELECT * FROM projects LIMIT 5;\"",
    "db:users:setup": "node scripts/d1-setup-users.js",
    "db:users:list": "wrangler d1 execute dubai-filmmaker-cms --remote --command=\"SELECT id, email, name, role FROM users;\"",
    "db:users:hash-passwords": "node scripts/hash-passwords.js",
    "r2:setup": "node scripts/r2-setup.js",
    "r2:list": "wrangler r2 object list dubai-filmmaker-assets",
    "r2:info": "wrangler r2 bucket list"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.952.0",
    "@aws-sdk/s3-request-presigner": "^3.952.0",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/list": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/bcrypt": "^6.0.0",
    "apexcharts": "^4.7.0",
    "autoprefixer": "^10.4.22",
    "bcrypt": "^6.0.0",
    "flatpickr": "^4.6.13",
    "next": "16.0.10",
    "next-auth": "^4.24.13",
    "react": "^19.2.0",
    "react-apexcharts": "^1.8.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "sonner": "^2.0.7",
    "swiper": "^11.2.10",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20.19.25",
    "@types/react": "^19.2.1",
    "@types/react-dom": "^19.2.1",
    "@types/react-transition-group": "^4.4.12",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.7",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "@react-jvectormap/core": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    },
    "@react-jvectormap/world": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    }
  }
}
